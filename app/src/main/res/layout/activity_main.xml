<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:background="#EAAD8C">
    <FrameLayout
            android:layout_width="411dp"
            android:layout_height="627dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:id="@+id/frameLayout" android:background="#ffffff">

    </FrameLayout>
    <TextView
            android:text="맛집 리스트"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnList"
            android:textColor="@color/white"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout"
            android:layout_marginStart="52dp"
            app:layout_constraintVertical_bias="0.512"/>
    <TextView
            android:text="즐겨찾기"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnLike"
            android:textColor="@color/white"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout"
            app:layout_constraintStart_toEndOf="@+id/btnList"
            app:layout_constraintEnd_toStartOf="@+id/btnPage"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintVertical_bias="0.512"/>
    <TextView
            android:text="내 정보"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnPage"
            android:textColor="@color/white"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout"
            android:layout_marginEnd="60dp"
            app:layout_constraintVertical_bias="0.512"/>
</androidx.constraintlayout.widget.ConstraintLayout>